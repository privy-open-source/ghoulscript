{
  "name": "@privyid/ghostscript",
  "description": "Ghostscript compiled to WebAssembly",
  "version": "0.1.0-alpha.1",
  "gsVersion": "10.03.1",
  "packageManager": "yarn@4.3.1",
  "type": "module",
  "main": "./dist/gs.cjs",
  "module": "./dist/gs.js",
  "types": "./dist/gs.d.ts",
  "exports": {
    ".": {
      "require": "./dist/gs.cjs",
      "import": "./dist/gs.js",
      "types": "./dist/gs.d.ts"
    },
    "./meta": {
      "require": "./dist/meta.cjs",
      "import": "./dist/meta.mjs",
      "types": "./dist/meta.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "prepare": "yarn build",
    "build": "./build.sh",
    "test": "node --test"
  },
  "devDependencies": {
    "@types/emscripten": "1.39.13",
    "vitest": "1.6.0"
  },
  "license": "AGPL-3.0-only",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/privy-open-source/ghoulscript",
    "directory": "packages/ghostscript"
  }
}
